{
  "name": "invoice-assist-frontend",
  "private": false,
  "version": "2.0.18",
  "description": "智能发票管理系统前端应用",
  "author": "Invoice Assist Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/invoice-assist-v2.git"
  },
  "keywords": [
    "invoice",
    "management",
    "react",
    "typescript",
    "supabase"
  ],
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:local": "vite --mode local",
    "dev:staging": "vite --mode staging",
    "dev:production": "vite --mode production",
    "build": "vite build",
    "build:local": "vite build --mode local",
    "build:staging": "vite build --mode staging",
    "build:production": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "preview": "vite preview",
    "deploy": "./scripts/deploy.sh",
    "deploy:prod": "./scripts/deploy.sh production",
    "clean": "rm -rf dist node_modules package-lock.json",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "env:switch": "node scripts/env-switch.js",
    "env:local": "node scripts/env-switch.js local",
    "env:staging": "node scripts/env-switch.js staging",
    "env:prod": "node scripts/env-switch.js prod",
    "env:check": "echo \"Current environment:\" && grep VITE_APP_ENV .env || echo \"No .env file found\"",
    "supabase:local": "supabase start && npm run env:local && npm run dev",
    "supabase:stop": "supabase stop",
    "supabase:status": "supabase status",
    "start:local": "npm run env:local && npm run dev",
    "start:staging": "npm run env:staging && npm run dev",
    "mobile:build": "npm run build && npx cap sync",
    "mobile:android": "npm run mobile:build && npx cap run android",
    "mobile:ios": "npm run mobile:build && npx cap run ios",
    "mobile:android:dev": "npx cap run android --livereload --external",
    "mobile:ios:dev": "npx cap run ios --livereload --external",
    "mobile:sync": "npx cap sync",
    "mobile:open:android": "npx cap open android",
    "mobile:open:ios": "npx cap open ios",
    "version:patch": "node scripts/update-version.cjs patch",
    "version:minor": "node scripts/update-version.cjs minor",
    "version:major": "node scripts/update-version.cjs major",
    "version:set": "node scripts/update-version.cjs custom",
    "prebuild": "node scripts/update-version.cjs patch",
    "build:release": "npm run version:patch && npm run build"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.3",
    "@capacitor/ios": "^7.4.3",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.50.3",
    "@tailwindcss/postcss": "^4.1.11",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-window": "^1.8.8",
    "@types/web-app-manifest": "^1.0.9",
    "@use-gesture/react": "^10.3.1",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "cally": "^0.8.0",
    "daisyui": "^5.0.43",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "jszip": "^3.10.1",
    "lucide-react": "^0.525.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-dropzone": "^14.3.8",
    "react-hot-toast": "^2.5.2",
    "react-loading-skeleton": "^3.5.0",
    "react-router-dom": "^7.6.3",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "react-window-infinite-loader": "^1.0.10",
    "recharts": "^3.1.2",
    "tailwindcss": "^4.1.11",
    "terser": "^5.43.1",
    "typescript": "~5.8.3",
    "vite": "^7.0.0",
    "vite-plugin-pwa": "^1.0.2",
    "workbox-window": "^7.3.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^7.4.3",
    "@capacitor/core": "^7.4.3",
    "@eslint/js": "^9.29.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "typescript-eslint": "^8.34.1"
  }
}
